        const ETCHING_BOUNDS = 10;

        /* ---------- DOM Elements (Cached) ---------- */
        const DOM = {
            runTag: document.getElementById('runTag'),
            flashOverlay: document.getElementById('flash-overlay'),
            tooltip: document.getElementById('tooltip'),
